{
  "name": "olive",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 5000 --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/cli-service": "^5.0.4",
    "acorn": "^8.7.1",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "bootstrap-vue": "^2.22.0",
    "core-js": "^3.22.8",
    "cropperjs": "^1.5.12",
    "ejs": "^3.1.8",
    "frontend": "^1.0.3",
    "install": "^0.13.0",
    "jquery": "^3.6.0",
    "minimist": "^1.2.6",
    "node-forge": "^1.3.1",
    "npm": "^8.12.1",
    "portfinder": "^1.0.28",
    "vue": "^3.2.37",
    "vue-router": "^4.0.15",
    "vue-template-compiler": "^2.6.14",
    "vue-upload-component": "^2.8.22"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.5.4",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.17.0",
    "eslint-plugin-vue": "^9.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "generator-star-spacing": "off",
      "semi": [
        2,
        "always"
      ],
      "camelcase": "off",
      "no-plusplus": "off",
      "eqeqeq": "off",
      "linebreak-style": 0
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
