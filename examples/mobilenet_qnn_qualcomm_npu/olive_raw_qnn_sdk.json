{
    "input_model":{
        "type": "OnnxModel",
        "config": {
            "model_path": "models/mobilenetv2-12.onnx"
        }
    },
    "evaluators": {
        "common_evaluator":{
            "metrics":[
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "sub_types": [{"name": "accuracy_score", "priority": 1, "metric_config": {"task": "multiclass", "num_classes": 1000}}],
                    "user_config":{
                        "user_script": "user_script.py",
                        "data_dir": "data/input",
                        "batch_size": 1,
                        "dataloader_func": "qnn_data_loader",
                        "post_processing_func": "post_process",
                        "inference_settings": {
                            "qnn":{
                                "backend": "libQnnCpu.so"
                            }
                        }
                    }
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [{"name": "avg", "priority": 2}],
                    "user_config":{
                        "user_script": "user_script.py",
                        "data_dir": "data/input",
                        "batch_size": 1,
                        "dataloader_func": "qnn_data_loader",
                        "inference_settings": {
                            "qnn":{
                                "backend": "libQnnCpu.so"
                            }
                        }
                    }
                }
            ]
        }
    },
    "passes": {
        "converter": {
            "type": "QNNConversion"
        },
        "build_model_lib": {
            "type": "QNNModelLibGenerator",
            "config": {
                "lib_targets": "x86_64-linux-clang"
            }
        }
    },
    "engine": {
        "log_severity_level": 0,
        "evaluator": "common_evaluator",
        "cache_dir": "cache",
        "output_dir": "models",
        "evaluate_input_model": false
    }
}
